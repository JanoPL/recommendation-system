{
  "name": "janopl/recommendation-system",
  "type": "project",
  "description": "Recommendation system",
  "license": "apache-2.0",
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5",
    "symfony/var-dumper": "^7.4",
    "squizlabs/php_codesniffer": "^4.0",
    "rector/rector": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Recommendations\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Recommendations\\Tests\\": "tests/",
      "Recommendations\\Tests\\Data\\": "tests/data/"
    }
  },
  "scripts": {
    "tests": "vendor/bin/phpunit tests"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
